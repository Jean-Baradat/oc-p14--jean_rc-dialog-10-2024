{
	"name": "@jean_b/rc-dialog",
	"private": false,
	"homepage": "https://Jean-Baradat.github.io/oc-p14--jean_rc-dialog-10-2024",
	"version": "1.1.0",
	"author": "Jean Baradat",
	"license": "MIT",
	"type": "module",
	"main": "./dist/rc-dialog.umd.cjs",
	"module": "./dist/rc-dialog.js",
	"exports": {
		".": {
			"import": "./dist/rc-dialog.js",
			"require": "./dist/rc-dialog.umd.cjs"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Jean-Baradat/oc-p14--jean_rc-dialog-10-2024.git",
		"directory": "packages/rc-dialog"
	},
	"scripts": {
		"dev": "vite",
		"dev:network": "vite --host",
		"tsc:noEmit": "tsc --noEmit --project tsconfig.app.json",
		"build:prod": "tsc --declaration && eslint . --max-warnings=0 && vite build",
		"build:dev": "tsc --noEmitOnError false && vite build --mode development",
		"build:demo": "tsc --declaration && eslint . --max-warnings=0 && vite build --config vite.demo.config.js",
		"build:demo:predeploy": "vite build --config vite.demo.config.js",
		"lint": "eslint .",
		"prettier:write": "prettier --write .",
		"preview": "vite preview --config vite.demo.config.ts",
		"preview:network": "vite preview --config vite.demo.config.ts --host",
		"predeploy": "npm run build:demo:predeploy",
		"deploy": "gh-pages -d demo",
		"publish:init": "npm publish --access public",
		"publish:patch": "npm version patch && npm publish",
		"publish:minor": "npm version minor && npm publish",
		"publish:major": "npm version major && npm publish"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE",
		"package.json"
	],
	"peerDependencies": {
		"lucide-react": "^0.407.0",
		"react": "^18.0.0"
	},
	"devDependencies": {
		"@emotion/css": "^11.13.4",
		"@emotion/styled": "^11.13.0",
		"@eslint/compat": "^1.2.0",
		"@eslint/js": "^9.9.0",
		"@radix-ui/react-slot": "^1.1.0",
		"@radix-ui/react-tabs": "^1.1.1",
		"@types/node": "^22.7.5",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.1",
		"@types/react-syntax-highlighter": "^15.5.13",
		"@vitejs/plugin-react": "^4.3.1",
		"autoprefixer": "^10.4.20",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"eslint": "^9.9.0",
		"eslint-plugin-react": "^7.37.1",
		"eslint-plugin-react-hooks": "^5.1.0-rc.0",
		"eslint-plugin-react-refresh": "^0.4.9",
		"eslint-plugin-tailwindcss": "^3.17.4",
		"gh-pages": "^6.2.0",
		"globals": "^15.9.0",
		"lucide-react": "^0.447.0",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"react": "^18.0.0",
		"react-dom": "^18.3.1",
		"react-syntax-highlighter": "^15.6.1",
		"simple-icons": "^13.15.0",
		"tailwind-merge": "^2.5.3",
		"tailwindcss": "^3.4.13",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.5.3",
		"typescript-eslint": "^8.0.1",
		"vite": "^5.4.1",
		"vite-plugin-dts": "^4.2.3"
	}
}
